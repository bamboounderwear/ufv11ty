{
  "name": "ufv-11ty-starter",
  "version": "1.0.0",
  "description": "11ty starter project for UFV",
  "main": "index.js",
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "sass --watch src/css:dist/css",
    "build:sass": "sass src/css:dist/css",
    "build:postcss": "postcss dist/css/*.css --replace",
    "build": "npm run build:sass && npm run build:postcss && eleventy",
    "start": "npm-run-all --parallel watch:*"
  },
  "keywords": ["11ty", "bootstrap", "ufv"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.0",
    "postcss-cli": "^8.3.0",
    "sass": "^1.43.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "bootstrap": "^5.1.0"
  }
}